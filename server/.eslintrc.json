{
	"env": {
		"browser": false,
		"node": true,
		"jest": true,
		"es2021": true
	},
	"extends": [
		"trybe-backend"
	],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"sonarjs/no-duplicate-string": ["error", 5],
		"max-params": [
      "error",
      5
    ],
		"max-lines-per-function": [
      "error",
      {
        "max": 100,
        "skipBlankLines": true,
        "skipComments": true
      }
    ]
	}
}
